{
    "getAll": {
        "description": "Gets all entries from 'sets'",
        "route": "/sets",
        "method": "GET",
        "sql": "SELECT * FROM sets",
        "auth_required": false,
        "allowed_roles": [
            "*"
        ]
    },
    "getById": {
        "description": "Gets an entry from 'sets' by its primary key",
        "route": "/sets/$set_id",
        "method": "GET",
        "sql": "SELECT * FROM sets WHERE set_id = $set_id",
        "auth_required": false,
        "allowed_roles": [
            "*"
        ]
    },
    "create": {
        "description": "Creates a new entry in 'sets'",
        "route": "/sets",
        "method": "POST",
        "sql": "INSERT INTO sets (match_id, winner_id, set_order, score) VALUES ($match_id, $winner_id, $set_order, $score)",
        "request_body_params": [
            "match_id",
            "winner_id",
            "set_order",
            "score"
        ],
        "auth_required": true,
        "allowed_roles": [
            "*"
        ]
    },
    "update": {
        "description": "Updates an existing entry in 'sets' by its primary key",
        "route": "/sets/$set_id",
        "method": "PUT",
        "sql": "UPDATE sets SET match_id = $match_id, winner_id = $winner_id, set_order = $set_order, score = $score WHERE set_id = $set_id",
        "request_body_params": [
            "match_id",
            "winner_id",
            "set_order",
            "score"
        ],
        "auth_required": true,
        "allowed_roles": [
            "*"
        ]
    },
    "delete": {
        "description": "Deletes an existing entry in 'sets' by its primary key",
        "route": "/sets/$set_id",
        "method": "DELETE",
        "sql": "DELETE FROM sets WHERE set_id = $set_id",
        "auth_required": true,
        "allowed_roles": [
            "*"
        ]
    }
}